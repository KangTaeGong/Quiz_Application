<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.leesangmin.quiz">

    <uses-permission android:name="android.permission.VIBRATE"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.android.leesangmin.quiz.soundActivity">
            <activity android:name="com.android.leesangmin.quiz.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.android.leesangmin.quiz.FirstActivity"/>
        <activity android:name="com.android.leesangmin.quiz.SecondActivity"/>
        <activity android:name="com.android.leesangmin.quiz.OneActivity"/>
        <activity android:name="com.android.leesangmin.quiz.bookActivity"/>
        <activity android:name="com.android.leesangmin.quiz.RandomActivity"/>
    </application>

</manifest>